# Configuração para coverage
[run]
source = app
omit = 
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */tests/*
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError

[html]
directory = htmlcov
